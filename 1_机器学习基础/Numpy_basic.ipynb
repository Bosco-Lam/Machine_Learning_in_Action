    "import numpy as np\n",
    "\n",
    "# create array\n",
    "array = np.random.rand(4,4)\n",
    "\n",
    "# array turn into matrix\n",
    "randMat = np.mat(array)\n",
    "\n",
    "# inverse matrix \n",
    "invRandMat = randMat.I\n",
    "\n",
    "# eye(4) create 4X4 Unit matrix\n",
    "myEye = randMat * invRandMat\n",
    "\n",
    "myEye - np.eye(4)"
   ]
  }
